SELECT COUNT(*),ACTION_END_DATE_201502,ACTION_END_DATE_201503,ACTION_END_DATE_201504,ACTION_END_DATE_201505
FROM A_1_ZHAOCANSHU 
WHERE lost_tag_5_t2_4g=1
GROUP BY ACTION_END_DATE_201502,ACTION_END_DATE_201503,ACTION_END_DATE_201504,ACTION_END_DATE_201505;

select count(*),lost_tag_5_t2_4g from A_1_ZHAOCANSHU where 
ACTION_END_DATE_201502=-1 and ACTION_END_DATE_201503=-1 and 
ACTION_END_DATE_201504=ACTION_END_DATE_201505 and ACTION_END_DATE_201504!=-1 and ACTION_END_DATE_201505!=-1
group by lost_tag_5_t2_4g;
